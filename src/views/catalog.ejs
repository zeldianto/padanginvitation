    <link href="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.10.5/viewer.min.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.10.5/viewer.min.js"></script>

    <style>
		.title-secondary {
            font-size: 17px;
            text-transform: capitalize;
            color: #7a7a7a;
        }

        .title-primary {
            font-size: 62px;
            font-weight: 900;
            color: #111;
            margin-top: 10px;
        }

		.page-heading {
            position: relative;
            text-align: center;
            padding-bottom: 100px;
        }

        .scroll-down {
            position: absolute;
            bottom: 0px;
            left: 50%;
            transform: translateX(-50%);
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        .scroll-down span {
            display: block;
            width: 30px;
            height: 30px;
            border-bottom: 2px solid #000;
            border-right: 2px solid #000;
            transform: rotate(45deg);
            margin: -10px;
            animation: scroll 1.5s infinite;
        }

        .scroll-down span:nth-child(2) {
            animation-delay: 0.3s;
        }

        .scroll-down span:nth-child(3) {
            animation-delay: 0.6s;
        }

        @keyframes scroll {
            0% {
                opacity: 0;
                transform: rotate(45deg) translate(-20px, -20px);
            }

            50% {
                opacity: 1;
            }

            100% {
                opacity: 0;
                transform: rotate(45deg) translate(20px, 20px);
            }
        }
    </style>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const whatsappButtons = document.querySelectorAll('.btn-whatsapp');
            const phoneNumber = '6283181887801'; // Your WhatsApp number

            whatsappButtons.forEach(function(button) {
                button.addEventListener('click', function(event) {
                    event.preventDefault();
					let message = "";
                    const model = button.getAttribute('data-model');
					if(model) {
						message = `Halo, saya tertarik dengan model ${model}`;
					} else {
						message = `Halo, saya mau tanya-tanya dulu seputar undangan pernikahan`;
					}
                    const whatsappURL = `https://wa.me/${phoneNumber}?text=${encodeURIComponent(message)}`;
                    window.open(whatsappURL, '_blank');
                });
            });
        });
    </script>

