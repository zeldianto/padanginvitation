		crossorigin="anonymous"></script>
	<link href="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.10.5/viewer.min.css"
		rel="stylesheet">
	<script src="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.10.5/viewer.min.js"></script>
	<script src="<%= THIS %>/static/assets/js/lib/simplyCountdown.min.js"></script>
	<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
	<script>
		AOS.init();
		const body = document.body;
		body.style.position = 'fixed';

		function closeNav() {
			document.getElementById('cover').style.height = '0%';
			let scrollY = body.style.top;
			body.style.position = '';
			body.style.top = '';
			window.scrollTo(0, parseInt(scrollY || '0') * -1);
			playMusic();
		}

		function playMusic() {
			const audio = document.querySelector("audio");
			audio.volume = 0.5;
			audio.play();
			document.getElementById("btn-play").style.display = "block";
			document.getElementById("btn-stop").style.display = "none";
		}

		function stopMusic() {
			const audio = document.querySelector('audio');
			audio.pause();
			document.getElementById('btn-play').style.display = 'none';
			document.getElementById('btn-stop').style.display = 'block';
		}

		const d = new Date(new Date().getTime() + 200 * 120 * 120 * 2000);
		simplyCountdown(".timer", {
			year: d.getFullYear(),
			month: d.getMonth() + 1,
			day: d.getDate()
		});

		document.addEventListener('DOMContentLoaded', function () {
			const gallery = new Viewer(document.getElementById('myGallery')); // ini untuk menampilkan semua gambar di dalam gallery
			gallery.forEach(function (image) {
				const viewer = new Viewer(image, {
					toolbar: true,
					navbar: false,
					title: false
				});
			});
		});

		document.addEventListener("contextmenu", function (e) {
			e.preventDefault();
		});

	</script>

</body>

