	</swiper-slide>
</swiper-container>
<div class="text-center pt-4 main-button-gradient">
	<a href="/catalog">Lihat lainnya</a>
</div>
<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-element-bundle.min.js"></script>
<style>
	
</style>
<script>
	const swiperEl = document.querySelector('swiper-container')
	Object.assign(swiperEl, {
		slidesPerView: 1,
		spaceBetween: 10,
		pagination: {
			clickable: true,
		},
		breakpoints: {
			"@0.00": {
				slidesPerView: 1,
				spaceBetween: 10,
			},
			"@0.75": {
				slidesPerView: 2,
				spaceBetween: 20,
			},
			"@1.00": {
				slidesPerView: 3,
				spaceBetween: 40,
			},
		},
	});

	swiperEl.initialize();
</script>
